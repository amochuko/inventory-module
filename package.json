{
  "name": "express",
  "version": "1.0.0",
  "description": "A demo api server",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "compile": "tsc",
    "start": "npm run compile && node ./dist/app.js",
    "start:prod": "npx tsx --env-file=.env src/app.ts",
    "dev:yoga": "NODE_ENV=development npx tsx --watch --env-file=.env src/graphql/yoga.ts",
    "predev:yoga": "npm run gql:codegen",
    "dev": "NODE_ENV=development npx tsx --watch --env-file=.env src/app.ts",
    "test": "jest",
    "gql:codegen": "graphql-codegen",
    "prisma:view": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "profiler": "NODE_ENV=production node --prof dist/app.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@envelop/graphql-modules": "^6.0.0",
    "@graphql-yoga/plugin-csrf-prevention": "^3.10.11",
    "@graphql-yoga/plugin-jwt": "^3.4.13",
    "@graphql-yoga/plugin-response-cache": "^3.12.11",
    "@parcel/watcher": "^2.5.1",
    "@whatwg-node/server-plugin-cookies": "^1.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-modules": "^2.4.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-yoga": "^5.10.10",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.12.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "0.11.0",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/graphql-modules-preset": "^4.0.13",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-resolvers": "^4.4.2",
    "@prisma/client": "6.0.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "prisma": "6.0.1",
    "ts-jest": "^29.2.5"
  }
}
